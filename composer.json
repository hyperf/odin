{
    "name": "hyperf/odin",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "",
    "autoload": {
        "psr-4": {
            "Hyperf\\Odin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Odin\\": "tests"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0",
        "hyperf/config": "~2.2.0 || 3.0.* || 3.1.*",
        "hyperf/di": "~2.2.0 || 3.0.* || 3.1.*",
        "hyperf/logger": "~2.2.0 || 3.0.* || 3.1.*",
        "hyperf/qdrant-client": "*",
        "justinrainbow/json-schema": "^6.3",
        "yethee/tiktoken": "^0.1.2"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.342",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": ">=7.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "suggest": {
        "swow/swow": "Required to create swow components."
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Hyperf\\Odin\\ConfigProvider"
        }
    }
}
